    "@devexpress/dx-react-core": "^2.4.1",
    "@devexpress/dx-react-scheduler": "^2.4.1",
    "@devexpress/dx-react-scheduler-material-ui": "^2.4.1",
    "@loadable/component": "^5.12.0",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "formik": "^2.1.4",
    "graphql-tag": "^2.10.3",
    "history": "^4.10.1",
    "material-ui-popup-state": "^1.5.3",
    "moment": "^2.24.0",
    "mui-datatables": "^2.14.0",
    "notistack": "^0.9.9",
    "ramda": "^0.27.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-google-recaptcha": "^2.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "yancey-js-util": "^1.1.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.1.4",
    "@types/loadable__component": "^5.10.0",
    "@types/mui-datatables": "^2.13.4",
    "@types/node": "^13.9.1",
    "@types/query-string": "^6.3.0",
    "@types/ramda": "^0.26.44",
    "@types/react": "^16.9.25",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-google-recaptcha": "^1.1.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/yup": "^0.26.32",
    "babel-plugin-transform-imports": "^2.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "graphql": "^14.6.0",
    "husky": "^4.0.7",
    "lint-staged": "^10.0.3",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.4.0",
    "standard-version": "^7.1.0",
    "typescript": "~3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
